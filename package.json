{
    "name": "terb",
    "version": "1.0.0",
    "license": "MIT",
    "homepage": "https://github.com/iamando/authfastapi#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/iamando/terb.git"
    },
    "bugs": {
        "url": "https://github.com/iamando/terb/issues"
    },
    "author": "iamando",
    "keywords": [
        "terb",
        "terminal",
        "web",
        "interface",
        "cli"
    ],
    "type": "module",
    "bin": {
        "terb": "cli.js"
    },
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "test": "xo && ava",
        "prepare": "husky install"
    },
    "files": [
        "cli.js",
        "ui.js"
    ],
    "dependencies": {
        "eslint": "^8.36.0",
        "import-jsx": "^5.0.0",
        "ink": "^3.2.0",
        "meow": "^9.0.0",
        "prettier": "^2.8.7",
        "react": "^17.0.2"
    },
    "ava": {
        "babel": true,
        "require": [
            "@babel/register"
        ]
    },
    "babel": {
        "presets": [
            "@babel/preset-env",
            "@babel/preset-react"
        ]
    },
    "xo": {
        "extends": "xo-react",
        "rules": {
            "react/prop-types": "off"
        }
    },
    "devDependencies": {
        "@ava/babel": "^2.0.0",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-react": "^7.18.6",
        "@babel/register": "^7.21.0",
        "@commitlint/cli": "^17.5.0",
        "@commitlint/config-conventional": "^17.4.4",
        "ava": "^5.2.0",
        "chalk": "^4.1.2",
        "eslint-config-prettier": "^8.8.0",
        "eslint-config-xo-react": "^0.27.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "ink-testing-library": "^3.0.0",
        "lint-staged": "^13.2.0",
        "validate-branch-name": "^1.3.0",
        "xo": "^0.39.1",
        "husky": "^8.0.0"
    },
    "lint-staged": {
        "*.js": [
            "eslint . --ext .js --fix",
            "prettier --write"
        ]
    },
    "validate-branch-name": {
        "pattern": "^(project-[a-z\\-]+|master|develop|preprod){1}$|^(epic|feature|bugfix|hotfix|chore|experiment|design)\\/([A-Z]{2,4})-([\\dX]+)_([\\w-]+?)$",
        "errorMsg": "Branch name format : <type>/<issue_ID>_<name>"
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ],
        "rules": {
            "type-enum": [
                2,
                "always",
                [
                    "epic",
                    "feature",
                    "bugfix",
                    "hotfix",
                    "chore",
                    "experiment",
                    "design"
                ]
            ]
        }
    }
}
